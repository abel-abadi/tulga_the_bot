<div style="display: flex; text-align: center;">
  <div style="flex: 1">
    <h1>I am tulga_the_bot.</h1>
  </div>
</div>
<div style="display: flex">
  <div style="flex: 1">
    <h3>Users</h3>
    <ul>
      {{#each users}}
        <li style="list-style-type: none">{{name}}</li>
      {{/each}}
    </ul>
  </div>
  <div style="flex: 1">
    <h3>Commands Usage:  @tulga_the_bot command</h3>
    <ul>
      {{#each commands}}
        <li style="list-style-type: none">{{this}}</li>
      {{/each}}
    </ul>
  </div>
  <div style="flex: 1">
    <h3>Channel</h3>
    <ul>
      {{#each channels}}
        <li style="list-style-type: none">{{name}}</li>
      {{/each}}
    </ul>
  </div>
</div>
